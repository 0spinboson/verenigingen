{
  "name": "verenigingen",
  "version": "1.0.0",
  "description": "Association Management App with TailwindCSS styling",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./verenigingen/templates/styles/tailwind.css -o ./verenigingen/public/css/tailwind.css --config ./tailwind.config.js --watch",
    "build": "tailwindcss -i ./verenigingen/templates/styles/tailwind.css -o ./verenigingen/public/css/tailwind.css --config ./tailwind.config.js --minify",
    "test": "jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:watch": "jest --watch --testMatch='**/tests/unit/**/*.test.js'",
    "test:coverage": "jest --coverage --testMatch='**/tests/unit/**/*.test.js'",
    "test:utilities": "jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:member": "cypress run --spec 'cypress/integration/member-*'",
    "test:e2e:payments": "cypress run --spec 'cypress/integration/*payment*,*sepa*'",
    "test:e2e:workflows": "cypress run --spec 'cypress/integration/workflows/**/*'",
    "test:complete": "npm run test:utilities && npm run test:e2e",
    "test:ci": "npm run test:utilities && cypress run --record",
    "test:debug:jest": "jest --runInBand --testMatch='**/tests/unit/**/*.test.js'",
    "test:debug:cypress": "cypress run --headed --no-exit",
    "test:api-contracts": "jest --testMatch='**/tests/unit/**/api-contract*.test.js'",
    "test:api-contracts:performance": "jest --testMatch='**/tests/unit/**/api-contract-performance.test.js' --verbose",
    "test:controllers": "jest --testMatch='**/tests/unit/**/test_*_controller_refactored.test.js'",
    "test:integration": "jest --testMatch='**/tests/unit/**/test_*_simple_contracts.test.js'",
    "validate-contract-coverage": "node scripts/validate-contract-coverage.js",
    "generate-contract-report": "node scripts/generate-contract-report.js",
    "quality-gate": "npm run test:api-contracts && npm run test:controllers && npm run test:integration",
    "lint": "eslint verenigingen --ext .js,.vue",
    "lint:fix": "eslint verenigingen --ext .js,.vue --fix",
    "format": "prettier --write \"**/*.{js,vue,json,css,scss,md}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "cypress run --record --parallel"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@cypress/code-coverage": "^3.12.0",
    "@tailwindcss/cli": "^4.1.10",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/cypress": "^10.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/vue": "^8.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "^29.0.0",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "babel-jest": "^29.0.0",
    "cypress": "^13.0.0",
    "cypress-file-upload": "^5.0.0",
    "cypress-real-events": "^1.10.0",
    "cypress-wait-until": "^3.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-frappe": "file:./eslint-plugins/eslint-plugin-frappe",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.33.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jest-junit": "^16.0.0",
    "msw": "^2.10.5",
    "nyc": "^15.0.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.10",
    "vue-eslint-parser": "^9.4.3"
  },
  "author": "Verenigingen Team",
  "license": "AGPL-3",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.77",
    "frappe-ui": "^0.1.168"
  }
}
